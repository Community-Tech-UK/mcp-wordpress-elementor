# WordPress site URL (without /wp-json/wp/v2/)
WORDPRESS_API_URL=https://your-site.com

# WordPress admin username
WORDPRESS_USERNAME=admin

# Application Password (generate in WordPress Admin > Users > Profile > Application Passwords)
WORDPRESS_PASSWORD=xxxx xxxx xxxx xxxx xxxx xxxx

# Optional: disable Elementor tools (default: enabled)
# DISABLE_ELEMENTOR=true

# Optional: disable Elementor global settings tools (default: enabled)
# DISABLE_ELEMENTOR_GLOBAL_SETTINGS=true
